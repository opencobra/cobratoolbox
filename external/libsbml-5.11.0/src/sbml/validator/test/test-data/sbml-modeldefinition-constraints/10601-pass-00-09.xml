<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="1">
<model name="algebraicRules_basic">
  <listOfCompartments>
    <compartment name="cell" volume="1"/>
  </listOfCompartments>
  <listOfSpecies>
    <specie name="x" compartment="cell" initialAmount="0.5"/>
    <specie name="y" compartment="cell" initialAmount="0.5"/>
    <specie name="z" compartment="cell" initialAmount="1"/>
  </listOfSpecies>
  <listOfRules>
    <algebraicRule formula="abs(z)"/>
  </listOfRules>
    <listOfReactions>
      <reaction name="R1" reversible="false">
        <listOfReactants>
          <specieReference specie="x"/>
        </listOfReactants>
        <listOfProducts>
          <specieReference specie="y"/>
        </listOfProducts>
        <kineticLaw formula="K_1*x">
          <listOfParameters>
			<parameter name="K_1" value="2"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
   </listOfReactions>
</model>
</sbml>
