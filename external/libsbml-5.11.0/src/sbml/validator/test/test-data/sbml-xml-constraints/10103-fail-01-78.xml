<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="1">
	<model>
		<notes>
			<body>
				<center><h2>A Simple Mitotic Oscillator</h2></center>
					<p>A minimal cascade model for the mitotic oscillator involving cyclin and cdc2 kinase</p>
			</body>
		</notes>
        <listOfCompartments>
			<compartment name="c"/>
        </listOfCompartments>
        <listOfSpecies>
        		<specie name="s" compartment="c" initialAmount="0"/>
        </listOfSpecies>
        <listOfReactions>
        		<reaction name="r">
        			<listOfProducts>
        				<specieReference specie="s"/>
        			</listOfProducts>
        		</reaction>
        </listOfReactions>
	</model>
</sbml>
