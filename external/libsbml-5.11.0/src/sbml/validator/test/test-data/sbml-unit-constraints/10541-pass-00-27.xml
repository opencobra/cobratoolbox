<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version4" level="2" version="4">
  <model>
    <listOfUnitDefinitions>
      <unitDefinition id="time">
        <listOfUnits>
          <unit kind="second" multiplier="60"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="substance">
        <listOfUnits>
          <unit kind="mole" scale="-2"/>
        </listOfUnits>
      </unitDefinition>
    </listOfUnitDefinitions>
    <listOfCompartments>
      <compartment id="c" units="litre" constant="false"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="s1" compartment="c" hasOnlySubstanceUnits="true"/>
      <species id="s2" compartment="c"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction id="R" reversible="false">
        <listOfReactants>
          <speciesReference species="s1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="s2"/>
        </listOfProducts>
        <kineticLaw>
          <listOfParameters>
            <parameter id="k_1" value="1" units="time"/>
            <parameter id="k2" value="1"/>
            <parameter id="k3" value="1" units="substance"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
