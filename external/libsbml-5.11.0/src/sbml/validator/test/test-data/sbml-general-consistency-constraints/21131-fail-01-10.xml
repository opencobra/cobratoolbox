<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version4" level="2" version="4">
  <model>
    <listOfCompartments>
      <compartment id="c"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="s" compartment="c"/>
      <species id="t" compartment="c"/>
      <species id="h" compartment="c"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction id="r">
        <listOfProducts>
          <speciesReference species="s">
            <stoichiometryMath>
              <math xmlns="http://www.w3.org/1998/Math/MathML">
                <apply>
                  <plus/>
                  <ci> s </ci>
                  <ci> h </ci>
                </apply>
              </math>
            </stoichiometryMath>
          </speciesReference>
        </listOfProducts>
        <listOfModifiers>
          <modifierSpeciesReference species="t"/>
        </listOfModifiers>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
