<?xml version="1.0" encoding="UTF-8"?>

<!--
Pass: (1405) A Species whose Compartment has spatialDimensions='1'
must have spatialSizeUnits of 'length', 'metre', or the id of a
UnitDefinition that defines a variant of 'metre' with exponent='1'.
-->

<sbml xmlns="http://www.sbml.org/sbml/level2" level="2" version="1">
  <model>
    <listOfCompartments>
      <compartment id="c" spatialDimensions="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="s" compartment="c" spatialSizeUnits="metre"/>
    </listOfSpecies>
  </model>
</sbml>
