<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version4" level="2" version="4">
  <model>
    <listOfCompartments>
      <compartment id="c"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="s" compartment="c" hasOnlySubstanceUnits="true"/>
      <species id="e" compartment="c"/>
      <species id="p" compartment="c"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction id="r">
        <listOfReactants>
          <speciesReference species="s"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="p"/>
        </listOfProducts>
        <listOfModifiers>
          <modifierSpeciesReference species="e"/>
        </listOfModifiers>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <ci> s </ci>
              <ci> k </ci>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="k" value="1" units="second" constant="false"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
