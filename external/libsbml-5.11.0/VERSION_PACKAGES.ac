##
## This file consists of simple definitions of variables about the current
## version of SBML L3 packages being included as part of the libSBML stable
## release.  The variables are currently not used for anything but
## documentation.

AC_DEFUN([CONFIG_SBML_PACKAGE_VARS],
[

# -----------------------------------------------------------------------------
# 'comp' package
# -----------------------------------------------------------------------------

AC_SUBST(PACKAGE_COMP_VERSION, 1)
AC_SUBST(PACKAGE_COMP_RELEASE, 3)
AC_SUBST(PACKAGE_COMP_SPEC_URL, [http://sbml.org/specifications/sbml-level-3/version-1/comp/sbml-comp-version-1-release-3.pdf])
AC_SUBST(PACKAGE_COMP_ERRATA_URL, [http://sbml.org/Documents/Specifications/SBML_Level_3/Packages/Hierarchical_Model_Composition_%28comp%29/Confirmed_issues_in_the_Hierarchical_Model_Composition_package_specification])

# -----------------------------------------------------------------------------
# 'fbc' package
# -----------------------------------------------------------------------------

AC_SUBST(PACKAGE_FBC_VERSION, 1)
AC_SUBST(PACKAGE_FBC_RELEASE, 1)
AC_SUBST(PACKAGE_FBC_SPEC_URL, [http://identifiers.org/combine.specifications/sbml.level-3.version-1.fbc.version-1.release-1])
AC_SUBST(PACKAGE_FBC_ERRATA_URL, [http://sbml.org/Documents/Specifications/SBML_Level_3/Packages/Flux_Balance_Constraints_%28flux%29/Confirmed_issues_in_the_Flux_Balance_Constraints_package_specification])

# -----------------------------------------------------------------------------
# 'layout' package
# -----------------------------------------------------------------------------

AC_SUBST(PACKAGE_LAYOUT_VERSION, 1)
AC_SUBST(PACKAGE_LAYOUT_RELEASE, 1)
AC_SUBST(PACKAGE_LAYOUT_SPEC_URL, [http://identifiers.org/combine.specifications/sbml.level-3.version-1.layout.version-1.release-1])
AC_SUBST(PACKAGE_LAYOUT_ERRATA_URL, [http://sbml.org/Documents/Specifications/SBML_Level_3/Packages/Layout_%28layout%29/Confirmed_issues_in_the_Layout_package_specification])

# -----------------------------------------------------------------------------
# 'qual' package
# -----------------------------------------------------------------------------

AC_SUBST(PACKAGE_QUAL_VERSION, 1)
AC_SUBST(PACKAGE_QUAL_RELEASE, 1)
AC_SUBST(PACKAGE_QUAL_SPEC_URL, [http://identifiers.org/combine.specifications/sbml.level-3.version-1.qual.version-1.release-1])
AC_SUBST(PACKAGE_QUAL_ERRATA_URL, [http://sbml.org/Documents/Specifications/SBML_Level_3/Packages/Qualitative_Models_%28qual%29/Confirmed_issues_in_the_Qualitative_Models_package_specification])

# -----------------------------------------------------------------------------
# The end.  Make sure to leave the text below this.
# -----------------------------------------------------------------------------

])
