<pkmkdoc spec="1.12">
  <properties>
    <title>LibSBML</title>
    <build>libSBML-@PACKAGE_VERSION@-@XML_PARSER@.pkg</build>
    <organization>org.sbml.libsbml</organization>
    <userSees ui="both" />
    <min-target os="3" />
    <domain anywhere="true" />
  </properties>
  <distribution>
    <versions min-spec="1.000000" />
    <scripts></scripts>
  </distribution>
  <description>Welcome to the installation program for libSBML
  @PACKAGE_VERSION@. In the following panels, you will be guided through the
  steps necessary to install the libSBML library and the language
  interfaces on your system.</description>
  <contents>
    <choice title="libSBML @PACKAGE_VERSION@" id="libsbml"
    tooltip="libSBML library files" starts_selected="true"
    starts_enabled="true" starts_hidden="false">
      <customLoc allowAltVol="true">/usr/local</customLoc>
      <pkgref id="org.sbml.libsbml.pkg" />
    </choice>
  </contents>
  <resources bg-scale="none" bg-align="left">
    <locale lang="en">
      <resource relative="true" mod="true" type="background">resources/background.png</resource>
      <resource relative="true" mod="true" type="license">resources/package-license.rtf</resource>
      <resource relative="true" mod="true" type="readme">resources/README.txt</resource>
      <resource relative="true" mod="true" type="welcome">resources/package-intro.txt</resource>
      <resource relative="true" mod="true" type="conclusion">resources/package-conclusion.rtf</resource>
    </locale>
  </resources>
  <flags />
  <item type="file">01local.xml</item>
  <mod>properties.customizeOption</mod>
  <mod>properties.userDomain</mod>
  <mod>properties.title</mod>
  <mod>description</mod>
  <mod>properties.systemDomain</mod>
</pkmkdoc>
