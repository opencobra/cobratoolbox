<xsd:schema targetNamespace="http://www.sbml.org/sbml/level1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.sbml.org/sbml/level1" elementFormDefault="qualified" version="$Version$">
