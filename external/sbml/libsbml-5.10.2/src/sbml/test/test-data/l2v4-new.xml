<?xml version="1.0" encoding="UTF-8"?><!-- edited with XMLSPY v2004 rel. 3 U (http://www.xmlspy.com) by Julian Weiss (University of Hertfordshire) --><sbml xmlns="http://www.sbml.org/sbml/level2/version4" level="2" version="4">  <model id="l2v4_all">    <listOfCompartments>      <compartment id="a" size="1" constant="false"/>    </listOfCompartments>    <listOfParameters>      <parameter id="p3" value="2" units="litre" constant="false"/>      <parameter id="x" value="2" units="dimensionless" constant="false"/>    </listOfParameters>    <listOfEvents>      <event useValuesFromTriggerTime="true">        <trigger>          <math xmlns="http://www.w3.org/1998/Math/MathML">            <apply>              <lt/>              <ci> x </ci>              <cn type="integer"> 3 </cn>            </apply>          </math>        </trigger>        <listOfEventAssignments>          <eventAssignment variable="a">            <math xmlns="http://www.w3.org/1998/Math/MathML">              <apply>                <times/>                <ci> x </ci>                <ci> p3 </ci>              </apply>            </math>          </eventAssignment>        </listOfEventAssignments>      </event>    </listOfEvents>  </model></sbml>
