Pass: (2005) A Species that does not specify its compartment
	cannot be represented in Level 1.
-->

<sbml xmlns="http://www.sbml.org/sbml/level2" level="2" version="1">
  <model>
    <listOfCompartments>
      <compartment id="c"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="s" compartment="c" hasOnlySubstanceUnits="false"
               initialConcentration="2.4"/>
    </listOfSpecies>
  </model>
</sbml>
