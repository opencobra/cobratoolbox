<?xml version="1.0" encoding="UTF-8"?>

<!--
Pass: (1406) A Species whose Compartment has spatialDimensions='2'
must have spatialSizeUnits of 'area' or the id of a UnitDefinition
that defines a variant of 'metre' with exponent='2'.
-->

<sbml xmlns="http://www.sbml.org/sbml/level2/version2" level="2" version="2">
  <model>
    <listOfCompartments>
      <compartment id="c" spatialDimensions="2"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="s" compartment="c" spatialSizeUnits="area"/>
    </listOfSpecies>
  </model>
</sbml>
