<?xml version="1.0" encoding="UTF-8"?>

<!--
Pass: (3002) The units returned by the assignment rule that assigns value
    to a parameter must be consistent with the units declared for
    that parameter provided units have been declared.
-->

<sbml xmlns="http://www.sbml.org/sbml/level2" level="2" version="1">
  <model>
    <listOfCompartments>
      <compartment id="c" units="litre" constant="false"/>
    </listOfCompartments>
    <listOfParameters>
      <parameter id="p" value="1" constant="false"/>
      <parameter id="p2" value="1" units="dimensionless"/>
    </listOfParameters>
    <listOfRules>
      <assignmentRule variable="p">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <ci> c </ci>
            <ci> p2 </ci>
          </apply>
        </math>
      </assignmentRule>
    </listOfRules>
  </model>
</sbml>
