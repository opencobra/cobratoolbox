<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="1">
    <model>
        <listOfCompartments>
			<compartment name="c"/>
        </listOfCompartments>
        <listOfSpecies>
        		<specie name="s" compartment="c" initialAmount="0"/>
        </listOfSpecies>
        <listOfParameters>
          <parameter name="p" value="2"/>
        </listOfParameters>
        <listOfReactions>
        		<reaction name="r">
        			<listOfProducts>
        				<specieReference specie="s"/>
        			</listOfProducts>
        			<kineticLaw formula="sum(s) *p"/>
        		</reaction>
        </listOfReactions>
    </model>
</sbml>
