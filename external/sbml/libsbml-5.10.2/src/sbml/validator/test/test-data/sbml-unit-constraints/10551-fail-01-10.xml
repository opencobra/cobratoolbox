<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version1/core" level="3" version="1">
  <model timeUnits="hour">
    <listOfUnitDefinitions>
      <unitDefinition id="minutes">
        <listOfUnits>
          <unit kind="second" exponent="1" scale="0" multiplier="60"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="hour">
        <listOfUnits>
          <unit kind="second" exponent="1" scale="0" multiplier="360"/>
        </listOfUnits>
      </unitDefinition>
    </listOfUnitDefinitions>
    <listOfParameters>
      <parameter id="p" value="1" units="minutes" constant="false"/>
      <parameter id="p1" value="1" units="minutes" constant="false"/>
      <parameter id="p2" value="1" units="dimensionless" constant="false"/>
      <parameter id="p3" value="1" units="dimensionless" constant="true"/>
    </listOfParameters>
    <listOfEvents>
      <event useValuesFromTriggerTime="true">
        <trigger persistent="true" initialValue="true">
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <lt/>
              <ci> p3 </ci>
              <cn> 0.1 </cn>
            </apply>
          </math>
        </trigger>
        <delay>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <abs/>
              <ci> p </ci>
            </apply>
          </math>
        </delay>
        <listOfEventAssignments>
          <eventAssignment variable="p2">
            <math xmlns="http://www.w3.org/1998/Math/MathML">
              <ci> p2 </ci>
            </math>
          </eventAssignment>
        </listOfEventAssignments>
      </event>
    </listOfEvents>
  </model>
</sbml>
