<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version4" level="2" version="4">
  <model>
    <listOfCompartments>
      <compartment id="c"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="s" compartment="c" boundaryCondition="true"/>
      <species id="x" compartment="c"/>
    </listOfSpecies>
    <listOfRules>
      <assignmentRule variable="s"/>
    </listOfRules>
    <listOfReactions>
      <reaction id="r">
        <listOfReactants>
          <speciesReference species="x"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="s"/>
        </listOfProducts>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
